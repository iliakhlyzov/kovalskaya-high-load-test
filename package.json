{
	"name": "kovalskaya-high-load-test",
	"version": "1.0.0",
	"main": "src/index.js",
	"type": "module",
	"scripts": {
		"dev": "nodemon src/index.js",
		"build": "yarn",
		"test": "NODE_OPTIONS=\"--experimental-vm-modules\" npx jest",
		"setup-hooks": "npx lefthook install",
		"start:with-migrations": "npm run migrate:up && npm run dev",
		"biome": "npx @biomejs/biome check --write .",
		"generate:migration": "npx sequelize-cli migration:generate --name",
		"migrate:up": "node scripts/migrate.js up",
		"migrate:down": "node scripts/migrate.js down",
		"migrate:pending": "node scripts/migrate.js pending",
		"migrate:executed": "node scripts/migrate.js executed"
	},
	"author": "Ilia Khlyzov",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"joi": "^17.13.3",
		"morgan": "^1.10.0",
		"pg": "^8.13.3",
		"sequelize": "^6.37.6",
		"umzug": "^3.8.2"
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/express": "^5.0.0",
		"@types/jest": "^29.5.14",
		"@types/joi": "^17.2.3",
		"@types/sequelize": "^4.28.20",
		"jest": "^29.7.0",
		"lefthook": "^1.11.3",
		"node-fetch": "^3.3.2",
		"nodemon": "^3.1.9",
		"sequelize-cli": "^6.6.2"
	}
}
